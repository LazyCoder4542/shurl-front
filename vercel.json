{
  "version": 2,
  "builds": [
    {
      "src": "api/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {"handle": "filesystem"},
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/:shortURL",
      "dest": "api/index.js"
    }
  ]
}
